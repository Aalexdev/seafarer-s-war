<?xml version="0.1.0" encoding="utf-8"?>

<clearEntitys/>

<setPlayer type="thunder" x="0" y="0" height="0" angle="40">

    <events>
        <keypad>
            <key tag="w" out="engineUp" type="push"/>
            <key tag="s" out="engineDown" type="push"/>
            <key tag="a" out="turnLeft" type="push"/>
            <key tag="d" out="turnRight" type="push"/>
            <key tag="UP" out="layerUp" type="sticky"/>
            <key tag="DOWN" out="layerDown" type="sticky"/>
            <key tag="l" out="lights"/>
        </keypad>
    </events>

    <part health="10"/>

    <equipments>
        <equipment name="MR-Elpis" id="0" angle="30"/>
        <equipment name="MR-Elpis" id="1" angle="30"/>
        <equipment name="MR-Elpis" id="2" angle="30"/>
        <equipment name="projector" id="3" angle="30"/>
        <equipment name="MR-Elpis" id="4" angle="30"/>
        <equipment name="MR-Elpis" id="5" angle="30"/>
        <equipment name="MR-Elpis" id="6" angle="30"/>
        <equipment name="MR-Elpis" id="7" angle="30"/>
        <equipment name="MR-Elpis" id="8" angle="30"/>
        <equipment name="MR-Elpis" id="9" angle="30"/>
    </equipments>

    <lights>
        <light type="15 white" x="16" y="53"/>
        <light type="15 white" x="16" y="88"/>
        <light type="15 white" x="16" y="110"/>
    </lights>

    <particles>
        <particle type="smoke 1" angle="90" range="40" x="17" y="151"/>
    </particles>
</setPlayer>

<summonEntity type="thunder" x="200" y="0" height="0" angle="0">
    <part health="10000"/>

    <equipments>
    </equipments>

    <lights>
        <light type="15 white" x="16" y="53"/>
        <light type="15 white" x="16" y="88"/>
        <light type="15 white" x="16" y="110"/>
    </lights>

</summonEntity>

<color r="0" g="0" b="0" a="255"/>